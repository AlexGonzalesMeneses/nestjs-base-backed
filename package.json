{
  "name": "base-backend-nest",
  "version": "1.0.2",
  "description": "",
  "author": "agetic@agetic.gob.bo",
  "private": true,
  "license": "LPG-Bolivia",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run check-node-version && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "setup": "rimraf database/migrations/*; npm run schema:drop;npm run migrations:generate init;npm run migrations:run;npm run seeds:run",
    "start": "nest start",
    "start:dev": "npm run check-node-version && NODE_ENV=development nest start --watch",
    "start:database": "./scripts/database.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "check-node-version": "node ./libs/check-node-version.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --coverage=false",
    "test:single": "NODE_ENV=test jest --coverage=false -i",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:generate": "npm run typeorm -- migration:generate -c connection -n",
    "migrations:run": "npm run typeorm -- migration:run -c connection",
    "migrations:revert": "npm run typeorm -- migration:revert -c connection",
    "seeds:create": "npm run typeorm -- migration:create -c seed -n",
    "seeds:run": "npm run typeorm -- migration:run -c seed",
    "schema:drop": "npm run typeorm -- schema:drop -c connection",
    "rimraf": "./node_modules/rimraf/bin.js",
    "compodoc": "compodoc -p tsconfig.json -o --language es-ES --disableCoverage --customLogo 'docs/imagenes/logoAgetic.png' --hideGenerator",
    "db:diagram": "ts-node ./node_modules/.bin/typeorm-uml ormconfig.ts -c connection -d docs/ERD.png",
    "prepare": "husky install",
    "pretest": "npm run check-node-version",
    "preinstall": "npm run check-node-version && mkdir -p node_modules; touch ./node_modules/.metadata_never_index"
  },
  "dependencies": {
    "@golevelup/ts-jest": "0.3.3",
    "@nestjs/axios": "0.0.8",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/schedule": "2.0.1",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.0.4",
    "@types/bcrypt": "5.0.0",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cls-rtracer": "2.6.0",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "dayjs": "1.11.3",
    "dotenv": "16.0.1",
    "ejs": "3.1.8",
    "express-session": "1.17.3",
    "file-stream-rotator": "0.6.1",
    "helmet": "5.1.0",
    "html-to-pdfmake": "2.4.2",
    "jsdom": "19.0.0",
    "nanoid": "3.3.4",
    "nest-authz": "2.2.0",
    "nestjs-pino": "2.6.0",
    "openid-client": "5.1.6",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pdfmake": "0.2.5",
    "pg": "8.7.3",
    "pino": "8.0.0",
    "pino-elasticsearch": "6.2.0",
    "pino-http": "8.0.1",
    "pino-http-send": "0.4.2",
    "pino-multi-stream": "6.0.0",
    "pino-pretty": "8.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "swagger-ui-express": "4.4.0",
    "typeorm": "0.2.45",
    "typeorm-adapter": "1.5.0",
    "typeorm-store": "2.0.0",
    "uuid": "8.3.2",
    "zxcvbn-typescript": "5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@compodoc/compodoc": "1.1.19",
    "@nestjs/cli": "8.2.6",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.7",
    "@types/cookie-parser": "1.4.3",
    "@types/cron": "2.0.0",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.4",
    "@types/jest": "28.1.2",
    "@types/node": "18.0.0",
    "@types/passport-local": "1.0.34",
    "@types/pdfmake": "0.1.21",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@types/zxcvbn": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "2.7.1",
    "supertest": "6.2.3",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.0",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.0.0",
    "typeorm-uml": "1.6.5",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "src/main.ts",
      "src/database/migrations/",
      "src/database/seeds/",
      ".module.ts",
      ".entity.ts",
      ".dto.ts",
      ".guard.ts",
      ".enum.ts"
    ],
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8.0.0 <8.6.0"
  }
}
